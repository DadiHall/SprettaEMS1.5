<div class="row well">
	
		<div class="col-md-3">
		 <%= line_chart [
  				{name: "Paper", data: current_user.papers.map{|t| [t.date, t.paper_weight] }, 'interpolateNulls':true},
  				{name: "EnvPaper", data: current_user.papers.map{|t| [t.date, t.env_paper_weight] }, 'interpolateNulls':true} ] %>
  				
	</div>

	<div class="col-md-3">
		 <%= line_chart [
  				{name: "KWST", data: current_user.electros.map{|t| [t.date, t.electricity_kwst] }, 'interpolateNulls':true},
  				{name: "Cost", data: current_user.electros.map{|t| [t.date, t.electricity_cost] }, 'interpolateNulls':true} ] %>
  				
	</div>
</div>
<div class="row">	
<div class="col-md-8 col-md-offset-2">
	<div class="panel-group" id="accordion">
		<div class="panel panel-default">
  			<div class="panel-heading">
    			<h3 class="panel-title"> 
    				<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapseAll"><span class="glyphicon glyphicon-minus"></span><%= current_user.name %>: Helstu tölur yfir auðlindanotkun</a></h3>
  			</div>
 			 <div class="panel-body">
			
			<div class="panel panel-default">
