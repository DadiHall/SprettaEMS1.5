    				<h3 class="panel-title"> 
    				<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapseOne"><span class="glyphicon glyphicon-minus"></span><%= current_user.name %>: Pappírs notkun</a> <%= link_to "Skrá Pappír", papers_path, class: 'pull-right ' %></h3>
  				</div>
  				    <div id="collapseOne" class="panel-collapse collapse in">

 			 <div class="panel-body">
				<div class="row">
					<%= render 'pages/partials/paper_part' %>
				
			</div>	
			</div>	
		</div>		
